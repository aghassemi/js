{
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "expr": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxlen": 80,
  "newcap": true,
  "noarg": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "sub": true,
  "trailing": true,
  "undef": true,
  "unused": "vars",

  "devel": true,
  "node": true,

  "globals": {
    "after": false,
    "afterEach": false,
    "assert": false,
    "before": false,
    "beforeEach": false,
    "describe": false,
    "expect": false,
    "it": false,
    "testconfig": false,
    "xdescribe": false,
    "xit": false
  }
}
