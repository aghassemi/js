{
  "name": "Veyron Extension",
  "version": "0.0.1",
  "description": "Access Veyron services from Javascript.",
  "manifest_version": 2,
  "icons": { "128": "images/icon_128.png" },
  "options_page": "options.html",
  "background": {
    "persistent": false,
    "scripts": [ "js/background.js" ]
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": [ "js/content.js" ]
  }],
  "permissions": [
    "<all_urls>",
    "identity",
    "storage"
  ],
  "web_accessible_resources": [
    "js/background.js.map",
    "js/content.js.map",
    "js/options.js.map"
  ],
  // See https://developer.chrome.com/apps/app_identity
  "oauth2": {
    "client_id": "761523829214-7s0ph21rs7pebc8eauvh9l0p12c6dnne.apps.googleusercontent.com",
    "scopes": [
      "email"
    ]
  },
  "key": ""
}

